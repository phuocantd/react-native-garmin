{
  "plugins": [
    ["module:react-native-dotenv"],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./src/components",
          "utils": "./src/utils",
          "hooks": "./src/hooks",
          "configs": "./src/configs"
        }
      }
    ]
  ]
}
